<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.attendancesystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AttendanceSystem">
        <activity
            android:name="com.example.attendancesystem.Change_Permission"
            android:exported="false" />
        <activity
            android:name="com.example.attendancesystem.Election_details"
            android:exported="false" />
        <activity
            android:name="com.example.attendancesystem.Del_Any"
            android:exported="false" />
        <activity
            android:name="com.example.attendancesystem.userSettings"
            android:exported="false" />
        <activity
            android:name="com.example.attendancesystem.CurrentElections"
            android:exported="false" />
        <activity
            android:name="com.example.attendancesystem.CheckPreviousAttendance"
            android:exported="false" />
        <activity
            android:name="com.example.attendancesystem.AddNewAttendance"
            android:exported="false" />
        <activity
            android:name="com.example.attendancesystem.Dashboard"
            android:exported="false" />
        <activity
            android:name="com.example.attendancesystem.Register"
            android:exported="false" />
        <activity
            android:name="com.example.attendancesystem.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>